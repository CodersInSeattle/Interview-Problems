#!/usr/bin/python
"""
var input = [1, {a: [2, [3]]}, 4, [5, [6]], [[7, ['hi']], 8, 9], 10];
var output = [1, {a: [2, [3]]}, 4, 5, 6, 7, 'hi', 8, 9, 10];
"""
def flattenArray(arr):
	res = []
#	print arr
	for item in arr:
		if type(item) == type([]):
			res += flattenArray(item)
		else:
			res.append(item)
	return res

print flattenArray([1, {"a": [2, [3]]}, 4, [5, [6]], [[7, ['hi']], 8, 9], 10])
